    <Platform Condition=" '$(Platform)' == '' ">AnyCPU</Platform>
    <ProductVersion>10.0.30319</ProductVersion>
    <SchemaVersion>2.0</SchemaVersion>
    <ProjectGuid>{991a5d82-4258-4fa9-ba66-e8dfd07c1e5e}</ProjectGuid>
